{
    "name": "depa/dot-navigation",
    "type": "library",
    "description": "DotKernel navigation component",
    "license": "MIT",
    "homepage": "https://github.com/dotkernel/dot-navigation",
    "keywords": [
        "navigation",
        "factories",
        "container",
        "laminas",
        "mezzio"
    ],
    "authors": [
        {
            "name": "depa",
            "email": "info@designpark.de"
        }
    ],
    "require": {
        "php": "^7.4 || ~8.1.0 || ~8.2.0",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "laminas/laminas-servicemanager": "^3.11.2",
        "laminas/laminas-escaper": "^2.10.0",
        "mezzio/mezzio-template": "^2.4.0",
        "dotkernel/dot-helpers": "^3.2.0",
        "laminas/laminas-dependency-plugin": "^2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "squizlabs/php_codesniffer": "^3.6.2",
        "laminas/laminas-stdlib": "^3.7.1",
        "dotkernel/dot-authorization": "^3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Navigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Navigation\\": "test/"
        }
    }
}
